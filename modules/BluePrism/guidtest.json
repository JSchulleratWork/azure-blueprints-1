{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "guid": {
            "type": "string",
            "metadata": {
                "description": "GUID for https://aka.ms/customerusageattribution."
            }
        },
        "templateGuid": {
            "type": "bool",
            "defaultValue": true,
            "metadata": {
                "description": "Control whether the empty pid-<GUID> resource is deployed. Set to false if exporting AZURE_HTPP_USER_AGENT."
            }
        },
        "test": {
           "type": "int",
           "metadata": {
                "description": "Test number"
            }
        }
    },
    "resources": [
        {
            "condition": "[parameters('templateGuid')]",
            "name": "[concat('pid-', parameters('guid'))]",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2018-02-01",
            "properties": {
                "mode": "Incremental",
                "template": {
                    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
                    "contentVersion": "1.0.0.0",
                    "resources": []
                }
            }
        },
        {
            "name": "[concat('test', parameters('test'), '-', parameters('guid'))]",
            "type": "Microsoft.Compute/disks",
            "apiVersion": "2018-06-01",
            "location": "westeurope",
            "sku": {
                "name": "StandardSSD_LRS"
            },
            "properties": {
                "osType": "Linux",
                "creationData": {
                    "createOption": "Empty"
                },
                "diskSizeGB": 128
            }
        }
    ],
    "outputs": {}
}